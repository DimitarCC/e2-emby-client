<setupxml>
	<setup key="e2embyclient" title="E2EmbyClient Setup">
		<item level="0" text="Timeouts"></item>
		<item level="0" text="---"></item>
		<item level="0" text="Connection reties count" description="Specify the connection retries count.">config.plugins.e2embyclient.conretries</item>
		<item level="0" text="Connection timeout" description="Specify the timeout for connect.">config.plugins.e2embyclient.con_timeout</item>
		<item level="0" text="Connection read timeout" description="Specify the timeout for connections read operations.">config.plugins.e2embyclient.read_con_timeout</item>
		<item level="0" text="---"></item>
		<item level="0" text="UI Settings"></item>
		<item level="0" text="---"></item>
		<item level="0" text="Thumb cache location" description="Specify where thumbnail cache will be storred.">config.plugins.e2embyclient.thumbcache_loc</item>
		<item level="0" text="UI scroll delay" description="Specify the delay between selection changed events.">config.plugins.e2embyclient.changedelay</item>
		<item level="0" text="Load poster for use with external plugins" description="Load poster in /tmp/emby/.... so to be able to be used from external plugins like LCD4Linux.">config.plugins.e2embyclient.storeposter</item>
		<item level="0" text="Add to Main Menu" description="Add E2EmbyClient to the Main Menu list of options.">config.plugins.e2embyclient.add_to_mainmenu</item>
		<item level="0" text="Add to Extensions Menu" description="Add E2EmbyClient to the Extensions Menu list of options.">config.plugins.e2embyclient.add_to_extensionmenu</item>
		<item level="0" text="---"></item>
		<item level="0" text="Player Settings"></item>
		<item level="0" text="---"></item>
		<item level="0" text="Playback system" description="Specify what will be the playback system. Possible choices is GStreamer/ExtePlayer3/HiPlayer.">config.plugins.e2embyclient.play_system</item>
		<item level="0" text="Encodding for non UTF-8 subtitles" description="Specify encodding that will be used to decode subtiles that are not UTF-8.">config.plugins.e2embyclient.encodding_nonutf_subs</item>
		<item level="0" text="---"></item>
		<item level="0" text="Connections"></item>
		<item level="0" text="---"></item>
	</setup>

	<setup key="e2embyclientconnection" title="E2EmbyClient Connection">
		<item level="0" text="Name" description="Name of the Remote Box (freely selectable).">self.entry.name</item>
		<item level="0" text="URL" description="Root URL address of the Connection.">self.entry.ip</item>
		<item level="0" text="Port" description="Port of the Connection.">self.entry.port</item>
		<item level="0" text="User" description="User for the Connection.">self.entry.user</item>
		<item level="0" text="Password" description="Password for the Connection.">self.entry.password</item>
	</setup>
</setupxml>
